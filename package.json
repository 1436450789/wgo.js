{
  "name": "wgo",
  "version": "3.0.0-alpha.2",
  "description": "JavaScript library for game of Go",
  "main": "index.js",
  "scripts": {
    "test": "mocha -R spec --compilers js:babel-register",
    "test-kifu": "mocha -R spec --compilers js:babel-register test/kifu.js",
    "install": "grunt",
    "build": "grunt",
    "docs": "grunt docs-prepare",
    "babel": "babel src/WGo.js -o src/WGo5.js",
    "start": "rollup -c rollup.config.js",
    "watch": "rollup -c rollup.config.js -w",
    "dev": "rollup -c rollup.config.js -w | http-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/waltheri/wgo.js.git"
  },
  "keywords": [
    "go",
    "board",
    "player"
  ],
  "author": "Jan Prokop <thebestgoplayer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waltheri/wgo.js/issues"
  },
  "homepage": "http://waltheri.github.io/wgo.js",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "chai": "^3.4.1",
    "eslint": "^3.14.0",
    "eslint-plugin-import": "^2.2.0",
    "grunt": "^0.4.5",
    "grunt-browserify": "^4.0.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-less": "^1.2.0",
    "grunt-contrib-uglify": "^0.9.2",
    "grunt-github-pages": "0.0.4",
    "grunt-jsdoc": "^1.0.0",
    "grunt-rollup": "^0.7.1",
    "grunt-string-replace": "^1.2.0",
    "http-server": "^0.9.0",
    "mocha": "^2.3.4",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {}
}
